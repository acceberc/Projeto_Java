package Familia44;

public class Celular extends Telefone {

	public Celular() {

		//super("Telefone Celular");

	}
	
	@Override
	public void toca(int codigoToque) {
		
		switch(codigoToque) {
		
		case 1:
			System.out.println("\nTãnãnã tãnãnã");
			break;
			
		case 2:
			System.out.println("\nPãnãnã");
			break;
			
		default:
			System.out.println("\nTinãnã");
			
		}
	}
	
	@Override
	public void disca(String numero) {
		
		System.out.println("\nO núemro " + numero + " é um celular");
	}

}
